Proceso Precedencia
	//Definicion de variables
	Definir a,b,c,d Como Entero;
	Definir x,i,z,w Como Real;
	
	//Inicializacion de variables
	a=3;
	b=5;
	c=2;
	d=4;
	x=2.0;
	i=3.5;
	z=5.0;
	w=1.2;
	
	//Manipulacion de variables
	Escribir "a*b/2+1 	= 8.5 = ",(a*b/2+1);
	Escribir "c/x^a 	= 2/8 = ",(c/x^a);
	Escribir "a/(b+c/(d+1)*(a+b)-a)^b^a+w = ",(a/(b+c/(d+1)*(a+b)-a)^b^a+w);
	Escribir "x/(b+c/(i+1)*(a+b)-a)^b^a+w ",(x/(b+c/(i+1)*(a+b)-a)^b^a+w);
FinProceso
