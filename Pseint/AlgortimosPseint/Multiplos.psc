Algoritmo Multiplos
	
	// Definicion de datos e inicializacion de variables.
	Definir  a,b Como Entero;
	// Lectura de datos.
	Escribir "Por favor ingrese el numero a:";
	Leer a;
	Escribir "Por favor ingrese el numero b:";
	Leer b;
	// Manipulacion de datos y Presentacion de datos.
	Si (a%b==0) Entonces
		Escribir "El numero a, es multiplo del numero b.";
	SiNo
		Escribir "El numero a, no es multiplo del numero b.";
	FinSi	
FinAlgoritmo
